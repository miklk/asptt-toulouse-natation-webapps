<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='asptt_toulouse_natation_app'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name="com.asptttoulousenatation.core.Core" />

  <inherits name="com.summatech.gwt.GwtTimepicker" />
  
  <inherits name='com.axeiya.gwtckeditor.Gwtckeditor'/>
  
  
<!-- <set-property name="user.agent" value="ie6,gecko1_8,safari,opera"/> -->
  <!-- Other module inherits                                      -->

  <!-- Specify the app entry point class.                         -->
  <entry-point class="com.asptttoulousenatation.client.Asptt_toulouse_natation_app" />

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>
  
  <!-- Use ClientFactoryImpl by default -->
  <replace-with class="com.asptttoulousenatation.client.config.ClientFactoryImpl">
    <when-type-is class="com.asptttoulousenatation.client.config.ClientFactory"/>
  </replace-with>
</module>
