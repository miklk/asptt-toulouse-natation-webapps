<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='asptt_toulouse_natation_app'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name="com.asptttoulousenatation.core.Core" />
  
  <inherits name='com.allen_sauer.gwt.dnd.gwt-dnd'/>

  <inherits name="com.summatech.gwt.GwtTimepicker" />
  
  <inherits name='com.axeiya.gwtckeditor.Gwtckeditor'/>
  
  <inherits name="gwtupload.GWTUpload"/>
  <!-- Load dynamically predefined styles in the library when the application starts -->
  <stylesheet src="Upload.css"/>
  
  
 <set-property name="user.agent" value="ie8,gecko1_8,safari"/>
  <!-- Other module inherits                                      -->

  <!-- Specify the app entry point class.                         -->
  <entry-point class="com.asptttoulousenatation.client.Asptt_toulouse_natation_app" />

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>
  
  <!-- Use ClientFactoryImpl by default -->
  <replace-with class="com.asptttoulousenatation.client.config.ClientFactoryImpl">
    <when-type-is class="com.asptttoulousenatation.client.config.ClientFactory"/>
  </replace-with>
  
  <replace-with class="com.asptttoulousenatation.client.ApplicationLaunch">
      <when-type-is class="com.asptttoulousenatation.client.ApplicationLaunch" />    
  </replace-with>
  <replace-with class="com.asptttoulousenatation.client.ApplicationLaunchIE7">
      <when-type-is class="com.asptttoulousenatation.client.ApplicationLaunch" />
      <when-property-is name="user.agent" value="ie6" />    
  </replace-with>
  
  <add-linker name="xsiframe"/>
<set-configuration-property name="devModeRedirectEnabled" value="true"/>

<set-configuration-property name='xsiframe.failIfScriptTag' value='FALSE'/>
</module>
